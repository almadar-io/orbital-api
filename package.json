    "url": "https://github.com/itechdom/playground/issues"
  },
  "homepage": "https://github.com/itechdom/playground#readme",
  "dependencies": {
    "@markab.io/node": "^9.5.0",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.11",
    "express": "^4.14.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.0"
  },
  "devDependencies": {
    "forever": "^4.0.3",
    "nodemon": "^3.1.0",
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
